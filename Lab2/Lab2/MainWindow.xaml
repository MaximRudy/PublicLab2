<Window x:Class="Lab2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab2"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Name="DeleteButton" Content="Delete" HorizontalAlignment="Left" Margin="612,340,0,0" VerticalAlignment="Top" Width="157" Height="40" Click="DeleteButton_Click"/>
        <Button Name="EditButton" Content="Edit" HorizontalAlignment="Left" Margin="612,266,0,0" VerticalAlignment="Top" Width="157" Height="40" Click="EditButton_Click"/>
        <Button Name="AddButton" Content="Add" HorizontalAlignment="Left" Margin="612,195,0,0" VerticalAlignment="Top" Width="157" Height="45" Click="AddButton_Click"/>
        <ListView Name="DesplayObjectListView" HorizontalAlignment="Left" Height="412" Margin="0,10,0,0" VerticalAlignment="Top" Width="584">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <ComboBox Name="ObjectSelector" HorizontalAlignment="Left" Margin="612,10,0,0" VerticalAlignment="Top" Width="157" Height="24"/>
    </Grid>
</Window>
